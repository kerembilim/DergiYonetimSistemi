@model IEnumerable<dys2.Models.Makale>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/BicimDenetleyici/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.YayinBasligi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ozet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.YazarName)
        </th>
        
       
       
       
        
        
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.YayinBasligi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ozet)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YazarName)
        </td>
       






        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Dosya Yükle", "DosyaYukle", new { id = item.Id }) |
            @Html.ActionLink("Dosya Ac", "DosyaAc", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>

}

</table>
